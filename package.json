{
  "name": "BookingEngine",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run start\" \"npm run ios\" \"npm run jsonServer\"",
    "dev:storybook": "concurrently \"npm run start\" \"npm run prestart\" \"npm run ios\"",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "jsonServer": "json-server --watch db.json --port 3004",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "reset": "npm start -- --reset-cache",
    "storybook-watcher": "sb-rn-watcher",
    "pod": "cd ios && pod install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/datetimepicker": "^6.3.2",
    "@react-native-community/slider": "^4.3.1",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@reduxjs/toolkit": "^1.8.5",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-backgrounds": "^5.3.23",
    "@storybook/addon-ondevice-controls": "^6.0.1-alpha.0",
    "@storybook/addon-ondevice-notes": "^5.3.26",
    "@storybook/react": "^6.5.10",
    "@storybook/react-native": "^6.0.1-beta.7",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^7.3.0",
    "json-server": "^0.17.0",
    "native-base": "^3.4.13",
    "react": "18.0.0",
    "react-native": "0.69.5",
    "react-native-calendars": "^1.1288.3",
    "react-native-gesture-handler": "^2.6.0",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^3.4.1",
    "react-native-screens": "^3.17.0",
    "react-native-svg": "^12.1.1",
    "react-redux": "^8.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@faker-js/faker": "^7.5.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^18.7.14",
    "@types/react-native": "^0.69.5",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
